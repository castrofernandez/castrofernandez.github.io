{
    "parser": "babel-eslint",
    "extends": "react",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "space-before-function-paren": 0,
        "operator-linebreak": [2, "after", { "overrides": { "?": "before", ":": "before" } }]
    }
}
